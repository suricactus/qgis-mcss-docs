        <definition>QVariant QgsFieldFormatter::createCache</definition>
        <argsstring>(QgsVectorLayer *layer, int fieldIndex, const QVariantMap &amp;config) const</argsstring>
        <name>createCache</name>
        <reimplementedby refid="classQgsValueRelationFieldFormatter_1a7d14539e34e86e3a095428db514a5c19">createCache</reimplementedby>
        <param>
          <type><ref refid="classQgsVectorLayer" kindref="compound">QgsVectorLayer</ref> *</type>
          <declname>layer</declname>
        </param>
        <param>
          <type>int</type>
          <declname>fieldIndex</declname>
