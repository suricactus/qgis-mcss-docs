        <type>Q_INVOKABLE <ref refid="classQgsQuickFeatureLayerPair" kindref="compound">QgsQuickFeatureLayerPair</ref></type>
        <definition>QgsQuickFeatureLayerPair QgsQuickIdentifyKit::identifyOne</definition>
